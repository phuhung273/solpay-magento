<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="solana" type="host">*.solana.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="github" type="host">https://raw.githubusercontent.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
